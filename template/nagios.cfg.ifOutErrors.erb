define service{
  use                 SNMP-service
  host_name           <%= @hostname %>
  service_description check_ifOutErrors_<%= @interface %>_<%= @ifIndex %>
  check_command       check_snmp_rate-multiplier!-o .1.3.6.1.2.1.2.2.1.20.<%= @ifIndex %> -l "ifOutErrors.<%= @ifIndex %>(<%= @interface %>)=" -u "packets" -c <%= @c_error_threshold %>
}

